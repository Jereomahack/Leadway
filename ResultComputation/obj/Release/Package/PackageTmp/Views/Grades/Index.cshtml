@model IEnumerable<LightWay.Models.Grades>

@{
    ViewBag.Title = "Index";
}


<div class="panel panel-primary">
    <div class="panel-heading">GRADES</div>
    <div class="panel-body">
        @*<p>
                @Html.ActionLink("Create New", "Create")
            </p>*@
        <table class="table table-striped table-bordered table-hover">
            <tr>
                <th>
                    @Html.DisplayNameFor(model => model.GradeName)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.GradePoint)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.MarkFrom)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.MarkUpto)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Note)
                </th>

            </tr>

            @foreach (var item in Model)
            {
                <tr>
                    <td>
                        @Html.DisplayFor(modelItem => item.GradeName)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.GradePoint)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.MarkFrom)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.MarkUpto)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Note)
                    </td>
                    @*<td>
                            @Html.ActionLink("Edit", "Edit", new { id = item.Id }) |
                            @Html.ActionLink("Details", "Details", new { id = item.Id }) |
                            @Html.ActionLink("Delete", "Delete", new { id = item.Id })
                        </td>*@
                </tr>
            }

        </table>
    </div>
</div>































