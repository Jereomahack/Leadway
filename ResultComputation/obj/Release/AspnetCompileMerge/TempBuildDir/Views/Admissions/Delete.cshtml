@model leadaway.Models.Admission


<div style="width:1050px">
    <div style="width:100px;float:left">
        <img src="~/Content/images/school logo.png" width="100" height="90" />
    </div>
    <div style="width:800px;float:left">
        <p style="text-align:center;color:red;font-size:40px;font-family:Arial, Helvetica, sans-serif"><b>@ViewBag.School</b></p>
        <p style="text-align:center;color:blue;font-size:20px;font-family:Courier New, Courier, monospace" class="textcolor"><b>@ViewBag.Address</b></>
        <p style="text-align:center;color:dodgerblue;font-size:12px;font-family:Courier New, Courier, monospace">@ViewBag.Others</p>
        @if (ViewBag.Category == "Pre-Nursery")
        {
            <p style="text-align:center;color:black;font-size:15px"><b>PRE-NURSERY SCHOOL CONTINUOUS ASSESSMENT  REPORT</b></p>
        }
        else if (ViewBag.Category == "Nursery")
        {
            <p style="text-align:center;color:black;font-size:15px"><b>NUSERY SCHOOL CONTINUOUS ASSESSMENT  REPORT</b></p>
        }
        else if (ViewBag.Category == "Primary")
        {
            <p style="text-align:center;color:black;font-size:15px"><b>PRIMARY SCHOOL CONTINUOUS ASSESSMENT  REPORT</b></p>
        }
        else if (ViewBag.Category == "Junior")
        {
            <p style="text-align:center;color:black;font-size:15px"><b>JUNIOR SECONDARY SCHOOL CONTINUOUS ASSESSMENT  REPORT</b></p>
        }
        else if (ViewBag.Category == "Senior")
        {
            <p style="text-align:center;color:black;font-size:15px"><b> SENIOR SECONDARY SCHOOL CONTINUOUS ASSESSMENT  REPORT</b></p>
        }
    </div>
    <div style="width:100px;float:left">
        @if (ViewBag.Passport != null)
        {

            <img src="data:image;base64,@System.Convert.ToBase64String( ViewBag.Passport)" width="90" height="90" class="img-thumbnail" />

        }
        else
        {
            <img src="#" width="120" height="120" class="img-thumbnail" />
        }
    </div>
    <br />
    <br />
</div>

