@model IEnumerable<LightWay.Models.ClassList>

@{
    ViewBag.Title = "Index";
}

<div class="panel panel-primary">
    <div class="panel-heading">Teacher Class</div>
    <div class="panel-body">
        <div class="row">
            <div class="col-lg-12">

                <table class="table table-striped table-bordered table-hover">
                    <tr>
                        <th>
                            Form Master
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.ClassName)
                        </th>
                    </tr>

                    @foreach (var item in Model)
                    {
                        <tr>
                            <td>
                                @Html.DisplayFor(modelItem => item.FormMaster)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.ClassName)
                            </td>

                        </tr>
                    }

                </table>
            </div>
        </div>

    </div>
</div>

