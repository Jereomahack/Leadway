@model IEnumerable<LightWay.Models.score>

@{
    ViewBag.Title = "IndexPreviousResult";
}






<link rel="stylesheet" type="text/css" media="print" href="~/Content/css/bootstrap.css">

<style type="text/css">
    p {
        margin: 0;
        padding: 0;
    }

    h1 {
        margin: 0;
        padding: 0;
    }

    table {
        border: 1px solid black;
        border-spacing: 0px 0px;
    }



    th {
        font-weight: bold;
        vertical-align: top;
        text-align: left;
        border: 1px solid black;
        padding: 2px;
    }

    td {
        vertical-align: top;
        text-align: left;
        border: 1px solid black;
        padding: 2px;
    }

    td2 {
        vertical-align: top;
        text-align: left;
        padding: 2px;
    }
</style>

<div>
    @if (ViewBag.NoDetails != null)
    {
        <div style="color:red">
            @ViewBag.NoDetails
        </div>
    }

    <div class="no-print">
        @using (Html.BeginForm("IndexPreviousResult", "Scores", FormMethod.Get))
        {
            <div class="row no-print container col-md-offset-2">

                @if (ViewBag.Role == "Parent")
                {
                    <div class="col-md-3 no-print">
                        <a> Admissin No(s): </a>  @Html.DropDownList("StudtNum", ViewBag.AdNum as SelectList, "Select Student Number", new { @class = "form-control" })
                    </div>
                }
                else
                {
                    <div class="col-md-3 no-print">
                        <a>Enter Admissin No: </a>    @Html.TextBox("StudtNum", ViewBag.CurrentFilter as string, new { @class = "form-control" })
                    </div>
                }

                <div class="col-md-3 no-print">
                    <br />
                    @Html.DropDownList("AccademicYear", (SelectList)ViewBag.AccademicYear, "Select Accademic Year", new { @class = "form-control" })
                </div>
                <div class="col-md-3 no-print">
                    <br />
                    @Html.DropDownList("Terms", (SelectList)ViewBag.Terms, "Select Term", new { @class = "form-control" })
                </div>
                <div class="col-md-3 no-print">
                    <br />
                    <input type="submit" value="Search" class="btn btn-primary " />
                </div>
            </div>
            <br />
            <br />

        }
    </div>
</div>


